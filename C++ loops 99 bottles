#include <iostream>
using namespace std;

int main()
{
for (int i = 99; i > 1; i--)
{
cout << i << " bottles of beer on the wall!" << i << " bottles of beer! Take one down, spin it around, " << i-1 << " bottles of beer on the wall!"
}

cout << "1 bottle of beer on the wall, 1 bottle of beer! Take it down, spin it around, no more bottles of beer on the wall!" << endl;

return 0;
}
